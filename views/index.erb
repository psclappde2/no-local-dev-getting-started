<div class="container">

  <table class="table table-striped table-bordered">
    <tr>
      <th>Number</th>
      <th>First Name</th>
      <th>Phone</th>
    </tr>

    <% @accounts.each do |account| %>
      <tr>
        <td><%= account.accountnumber %></td>
        <td><%= account.name %></td>
        <td><%= account.phone %></td>
      </tr>
    <% end %>
  </table>

</div>
