<div class="container">

  <table class="table table-striped table-bordered">
    <tr>
      <th>Number</th>
      <th>First Name</th>
      <th>Phone</th>
    </tr>

    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.phone %></td>
        <td><%= contact.email %></td>
      </tr>
    <% end %>
  </table>

</div>
